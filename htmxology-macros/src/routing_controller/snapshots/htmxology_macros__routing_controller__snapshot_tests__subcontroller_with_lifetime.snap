    impl<
        '_component_spec_lifetime,
    > htmxology::HasSubcontroller<
        '_component_spec_lifetime,
        &'_component_spec_lifetime DataController,
        (),
    > for AppController {
        fn as_subcontroller(
            &'_component_spec_lifetime self,
            args: (),
        ) -> &'_component_spec_lifetime DataController {
            self.into()
        }
        fn convert_response(
            htmx: &htmxology::htmx::Request,
            response: <&'_component_spec_lifetime DataController as htmxology::Controller>::Response,
        ) -> <Self as htmxology::Controller>::Response {
            response.into()
